@data =
    SELECT * FROM SubjPropObj;

@stmtFilter =
    SELECT DISTINCT [Subj]
    FROM @data
    WHERE Prop == "P279"; //subclassOf

//This one is for sampling
/*
@stmtSample=
    SELECT [Subj]
    FROM @stmtFilter
         SAMPLE ANY(5);
*/

//This one is to count 
@stmtCount=
    SELECT
        COUNT() AS NumTypes
        FROM @stmtFilter;
           

OUTPUT @stmtCount
TO "/wikidata/taxonomiai-tipus-darabszam.csv"
USING Outputters.Csv(outputHeader:true,quoting:true);