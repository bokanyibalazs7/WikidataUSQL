@data =
    SELECT * FROM SubjPropObj;

@stmtFilter=
    SELECT DISTINCT [Obj]
    FROM @data
    WHERE Prop == "P31"; //instace of



//This is for sampling
/*
@stmtSample=
    SELECT [Obj]
    FROM @stmtFilter
         SAMPLE ANY(100);
*/

@stmtCount=
    SELECT
        COUNT() AS NumType
    FROM @stmtFilter;


OUTPUT @stmtCount
TO "/wikidata/hivatkozott-tipus-darabszam.csv"
USING Outputters.Csv(outputHeader:true,quoting:true);